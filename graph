digraph {
	dummy [shape=point]
	q0 [shape=circle]
	dummy -> q0
	q1 [shape=circle]
	q2 [shape=doublecircle]
	q3 [shape=circle]
	q4 [shape=doublecircle]
	q0 -> q1 [label=a]
	q1 -> q1 [label=a]
	q1 -> q2 [label=b]
	q2 -> q2 [label=b]
	q2 -> q1 [label=a]
	q0 -> q3 [label=b]
	q4 -> q2 [label=b]
	q4 -> q3 [label=a]
	q3 -> q3 [label=a]
	q3 -> q3 [label=b]
}
